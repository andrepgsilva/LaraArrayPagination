{
    "name": "andrepgsilva/lara-array-pagination",
    "description": "Make Laravel Pagination works with array",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Silva",
            "email": "andrepgsilva@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Andrepgsilva\\LaraArrayPagination\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andrepgsilva\\LaraArrayPagination\\Providers\\LaraArrayPaginationServiceProvider"
            ],
            "dont-discover": [
                "*"
            ]
        }
    },
    "require": {
    	"php": "^7.3",
        "illuminate/pagination": ">=5.1.1",
        "illuminate/support": ">=5.1.1"
    },
    "require-dev": {
        "laravel/framework": "^8.11",
        "orchestra/testbench": "^6.3"
    }
}
